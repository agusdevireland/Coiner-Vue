{
  "manifest_version": 3,
  "name": "Coiner",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/icons/icon16.jpg",
      "48": "assets/icons/icon48.jpg",
      "128": "assets/icons/icon128.jpg"
    }
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["<all_urls>"], 
      "js": ["content.js"]      
    }
  ]
}



  